<?xml version='1.0'?>
<database>

    <table name="innomedia_media">
        <field name="id" type="integer" notnull="1"/>
        <field name="page" type="text" />
        <field name="pageid" type="integer" />
        <field name="block" type="text" />
        <field name="blockcounter" type="integer" />
        <field name="name" type="text" notnull="1" />
        <field name="path" type="text" />
        <field name="filetype" type="text" />
        <field name="fileid" type="text" />
        <key field="id"  type="unique"/>
    </table>

    <sequence name="innomedia_media_id_seq"/>

</database>

